/*top 5 videos*/

* html #allTabs a span {cursor: pointer;}

#topFive {
	width:300px;
	background-color:#fff;
	margin:10px 0px;
}

#topFive[id] {
	background-color:transparent;
	background-image:url(./images/75p.png);
}


#topFive #tabs {width:300px; margin: 0 auto;}

#topFive h2 span {display:none;}

#tabFooter {
	width:300px; 
	height:0px;
	line-height:0;
	border-top:4px solid #b395c0;
	clear:both;
	margin:0px;
	padding:0px;
}


#topFive #tabs #allTabs img {
	float:left;
	margin-right:8px;
}

#topFive #tabs #allTabs ul {
	border-top: 4px solid #b395c0;
	margin:0px;
	padding:0px;
}

#topFive #tabs #allTabs li {
	border-top: 4px solid #dcdcdc;
	margin:0px;
	padding:0px;
	clear:both;
}

#topFive #tabs #allTabs li:first-child {
	border-top: none;
}

#topFive #tabs #allTabs a {
	display:block;
	width:290px;
	/*height:35px;*/
	padding:5px;
}

#topFive #tabs #allTabs a:hover {
		background-color:#eaeaea;
		text-decoration:none;
		
}

#topFive #tabs #allTabs .tabCopy {
	width:210px;
	float:left;
	display:block;
	font-size:11px;
	color:#000;
	font-style: italic;
}

#topFive #tabs #allTabs .tabCopyHeader {
	font-size:12px;
	font-weight:bold;
	display:block;
	width:210px;
	float:left;
		color:#333;
	font-weight:bold;
}

#topFive #tabs #allTabs .number {
	color:#f9dc30;
	font-size:22px;
	float:right;
	display:block;
	width: 17px;
	height:35px;
	padding: 3px 0 0 7px;
	font-weight:bold;
}

#topFive #tabs #allTabs a:hover .number {
color:#ffb400;
}


#topFive #tabs #tabHeader {
	position:relative;
	height:70px; 
	background: url(./images/header_bg.jpg) 0 0 no-repeat;
}

