<script type="text/javascript">
var $jq126 = jQuery.noConflict(); // to be removed
$jq126(document).ready(function(){
var flagdrupal = true;
var slideStart = false;

onBefore = function(curr, next, opts) {
if($jq126('#promo6')) {
  if(!slideStart) {
    var nowNav = $jq126(opts.promoOneNav).children('div:eq('+opts.currSlide+')').addClass('activeSlide');
    slideStart = true;
    return;
    }
  var nextNav = $jq126(opts.promoOneNav).children('div:eq('+opts.nextSlide+')');
  $jq126(opts.promoOneNav).children('div:eq('+opts.currSlide+')').removeClass('activeSlide');
  nextNav.addClass('activeSlide');
} else {
  setTimeout("onBefore()", 2000 );
  }
}

$jq126('#cycleMeNow').cycle({
fx: 'fade',
speed: 1000,
timeout: 5000,
pause: 1,
pager: '#promoOneNav',
startingSlide: 0,
pagerAnchorBuilder: function(i,o) {
var src = $jq126('img',o).attr('src'); 
src = src.replace('.jpg', '_b.jpg');
return $jq126('<div id="promo_'+(i+1)+'"><a href="#"><span class="arrow">&nbsp;</span><img src="' + src + '" width="78" height="42" /></a></div>');
},
before: onBefore
});



$jq126('div#pauseButton') .click(function(){
if ($jq126('div#pauseButton') .hasClass('notpaused')){
	$jq126('#cycleMeNow').cycle('pause');
	$jq126('div#pauseButton') .css('background-image','url(/images/hp/resume_78x42.gif)');
	$jq126('div#pauseButton') .removeClass('notpaused');
	$jq126('div#pauseButton') .addClass('paused');
} else if ($jq126('div#pauseButton') .hasClass('paused')){
	$jq126('#cycleMeNow').cycle('resume');
	$jq126('div#pauseButton') .css('background-image','url(/images/hp/pause_78x42.gif)');
	$jq126('div#pauseButton') .removeClass('paused');
	$jq126('div#pauseButton') .addClass('notpaused');
};
});
}); // end document ready function
</script>


